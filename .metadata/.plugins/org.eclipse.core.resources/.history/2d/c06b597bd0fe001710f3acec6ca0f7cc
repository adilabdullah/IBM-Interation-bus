

CREATE COMPUTE MODULE DatabaseConnection_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		-- CALL CopyEntireMessage();
	--	INSERT INTO Database.{oracle_dsn}.{alhamd}.{simple1} (sno,name,age,salary,company) values (1,'adil',15,65.32,'bank alfalah');
		INSERT INTO 'alhamd'.'simple1' (sno,name,age,salary,company) values (1,'adil',15,65.32,'bank alfalah');
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
